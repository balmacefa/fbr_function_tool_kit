    "file-regex": "^3.1.0",
    "inquirer": "^9.2.12",
    "json2md": "^2.0.0",
    "langchain": "^0.0.204",
    "localtunnel": "^2.0.2",
    "morgan": "^1.10.0",
    "navalia": "^1.3.0",
    "ngrok": "5.0.0-beta.2",
    "puppeteer": "^21.6.0",
    "puppeteer-extra": "^3.3.6",
    "readdirp": "^3.6.0",
    "remarkable": "^2.0.1",
    "superjson": "^2.2.1",
    "swagger-ui-express": "^5.0.0",
    "ts-md5": "^1.3.1",
    "ts-morph": "^21.0.1",
