{
  "name": "fbr_function_tool_kit",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev:updates_check": "npx ncu",
    "dev:updates_apply": "npx ncu -u",
    "build": "tsup src/server/index.ts --format cjs,esm --dts",
    "release": "pnpm run build && changeset publish",
    "_dev:server": "tsx watch src/server/index.ts",
    "dev:server_taskmaster": "tsx watch src/server/AgentOpenAI/TaskMasterAI/server.ts",
    "dev:client": "wait-port 2022 && tsx watch src/client",
    "dev": "tsx watch src/server/AgentOpenAI/TaskMasterAI/server.ts",
    "_dev": "run-p dev:* --print-label",
    "gen:openapi": "npx openapicmd typegen ./docs.json > src/openapi.d.ts",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives src",
    "type-check": "tsc",
    "start:server": "node dist/server/index.js",
    "test": "jest",
    "start:client": "wait-port 2022 && node dist/client/index.js",
    "start": "run-p start:* --print-label",
    "test-dev": "start-server-and-test 'tsx src/server' http-get://localhost:2022 'tsx src/client'",
    "test-start": "start-server-and-test 'node dist/server' http-get://localhost:2022 'node dist/client'"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.0",
    "common-es": "^1.0.1",
    "directory-tree": "^3.5.1",
    "json2md": "^2.0.0",
    "navalia": "^1.3.0",
    "readdirp": "^3.6.0",
    "remarkable": "^2.0.1",
    "superjson": "^2.2.1",
    "ts-md5": "^1.3.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "tsup": "^8.0.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.3",
    "@types/remarkable": "^2.0.8",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "esbuild": "^0.19.8",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.0.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "wait-port": "^1.1.0"
  },
  "peerDependencies": {
    "openai": "^4.20.0",
    "openapi-client-axios": "^7.4.0",
    "@fastify/websocket": "^8.2.0",
    "@trpc/client": "^10.44.0",
    "@trpc/server": "^10.44.0",
    "fastify": "^4.24.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}