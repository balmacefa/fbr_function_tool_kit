generator client {
  provider = "prisma-client-js"
  output   = "./primas_client_chat"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model FBR_ChatSessionData {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  userId      String  @db.ObjectId
  title       String?
  threadId    String?
  assistantId String
}
