      <svg
        stroke="currentColor"
        fill="none"
        stroke-width="0"
        viewBox="0 0 15 15"
        class="h-6 w-6 text-white"
        height="1em"
        width="1em"
        xmlns="http://www.w3.org/2000/svg"
