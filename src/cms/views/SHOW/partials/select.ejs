<!-- select.ejs -->
<div class="form-group">
  <label class="block text-gray-700 text-sm font-bold mb-2" for="<%= field.data_path %>">
    <%= field.label %>
  </label>
  <select id="<%= field.data_path %>" name="<%= field.data_path %>" class="form-select w-full border border-gray-300 rounded">
    <% field.options.forEach(option => { %>
    <option value="<%= option.value %>"><%= option.label %></option>
    <% }); %>
  </select>
</div>
